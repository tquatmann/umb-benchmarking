module MAPK
	k: [0..N] init N;
	k_kkpp: [0..N] init 0;
	kp: [0..N] init 0;
	kp_kkpp: [0..N] init 0;
	kp_ptase: [0..N] init 0;
	kpp: [0..N] init 0;
	kpp_ptase: [0..N] init 0;
	<a_k_kk> ((k > 0) & (k_kkpp < N)) -> (a7 * k) : (k' = (k - 1)) & (k_kkpp' = (k_kkpp + 1));
	<d_k_kk> ((k < N) & (k_kkpp > 0)) -> (d7 * k_kkpp) : (k' = (k + 1)) & (k_kkpp' = (k_kkpp - 1));
	<k_k_kk> ((k_kkpp > 0) & (kp < N)) -> (k7 * k_kkpp) : (k_kkpp' = (k_kkpp - 1)) & (kp' = (kp + 1));
	<a_k_ptase> ((kp > 0) & (kp_ptase < N)) -> (a8 * kp) : (kp' = (kp - 1)) & (kp_ptase' = (kp_ptase + 1));
	<d_k_ptase> ((kp < N) & (kp_ptase > 0)) -> (d8 * kp_ptase) : (kp' = (kp + 1)) & (kp_ptase' = (kp_ptase - 1));
	<k_k_ptase> ((kp_ptase > 0) & (k < N)) -> (k8 * kp_ptase) : (k' = (k + 1)) & (kp_ptase' = (kp_ptase - 1));
	<a_k_kk> ((kp > 0) & (kp_kkpp < N)) -> (a9 * kp) : (kp' = (kp - 1)) & (kp_kkpp' = (kp_kkpp + 1));
	<d_k_kk> ((kp < N) & (kp_kkpp > 0)) -> (d9 * kp_kkpp) : (kp' = (kp + 1)) & (kp_kkpp' = (kp_kkpp - 1));
	<k_k_kk> ((kp_kkpp > 0) & (kpp < N)) -> (k9 * kp_kkpp) : (kp_kkpp' = (kp_kkpp - 1)) & (kpp' = (kpp + 1));
	<a_k_ptase> ((kpp > 0) & (kpp_ptase < N)) -> (a10 * kpp) : (kpp' = (kpp - 1)) & (kpp_ptase' = (kpp_ptase + 1));
	<d_k_ptase> ((kpp < N) & (kpp_ptase > 0)) -> (d10 * kpp_ptase) : (kpp' = (kpp + 1)) & (kpp_ptase' = (kpp_ptase - 1));
	<k_k_ptase> ((kpp_ptase > 0) & (kp < N)) -> (k10 * kpp_ptase) : (kp' = (kp + 1)) & (kpp_ptase' = (kpp_ptase - 1));
endmodule

module MAPKK
	kk: [0..N] init N;
	kk_kkkp: [0..N] init 0;
	kkp: [0..N] init 0;
	kkp_kkkp: [0..N] init 0;
	kkp_ptase: [0..N] init 0;
	kkpp: [0..N] init 0;
	kkpp_ptase: [0..N] init 0;
	<a_kk_kkk> ((kk > 0) & (kk_kkkp < N)) -> (a3 * kk) : (kk' = (kk - 1)) & (kk_kkkp' = (kk_kkkp + 1));
	<d_kk_kkk> ((kk < N) & (kk_kkkp > 0)) -> (d3 * kk_kkkp) : (kk' = (kk + 1)) & (kk_kkkp' = (kk_kkkp - 1));
	<k_kk_kkk> ((kk_kkkp > 0) & (kkp < N)) -> (k3 * kk_kkkp) : (kk_kkkp' = (kk_kkkp - 1)) & (kkp' = (kkp + 1));
	<a_kk_ptase> ((kkp > 0) & (kkp_ptase < N)) -> (a4 * kkp) : (kkp' = (kkp - 1)) & (kkp_ptase' = (kkp_ptase + 1));
	<d_kk_ptase> ((kkp < N) & (kkp_ptase > 0)) -> (d4 * kkp_ptase) : (kkp' = (kkp + 1)) & (kkp_ptase' = (kkp_ptase - 1));
	<k_kk_ptase> ((kkp_ptase > 0) & (kk < N)) -> (k4 * kkp_ptase) : (kk' = (kk + 1)) & (kkp_ptase' = (kkp_ptase - 1));
	<a_kk_kkk> ((kkp > 0) & (kkp_kkkp < N)) -> (a5 * kkp) : (kkp' = (kkp - 1)) & (kkp_kkkp' = (kkp_kkkp + 1));
	<d_kk_kkk> ((kkp < N) & (kkp_kkkp > 0)) -> (d5 * kkp_kkkp) : (kkp' = (kkp + 1)) & (kkp_kkkp' = (kkp_kkkp - 1));
	<k_kk_kkk> ((kkp_kkkp > 0) & (kkpp < N)) -> (k5 * kkp_kkkp) : (kkp_kkkp' = (kkp_kkkp - 1)) & (kkpp' = (kkpp + 1));
	<a_kk_ptase> ((kkpp > 0) & (kkpp_ptase < N)) -> (a6 * kkpp) : (kkpp' = (kkpp - 1)) & (kkpp_ptase' = (kkpp_ptase + 1));
	<d_kk_ptase> ((kkpp < N) & (kkpp_ptase > 0)) -> (d6 * kkpp_ptase) : (kkpp' = (kkpp + 1)) & (kkpp_ptase' = (kkpp_ptase - 1));
	<k_kk_ptase> ((kkpp_ptase > 0) & (kkp < N)) -> (k6 * kkpp_ptase) : (kkp' = (kkp + 1)) & (kkpp_ptase' = (kkpp_ptase - 1));
	<a_k_kk> (kkpp > 0) -> kkpp : (kkpp' = (kkpp - 1));
	<d_k_kk> (kkpp < N) -> 1 : (kkpp' = (kkpp + 1));
	<k_k_kk> (kkpp < N) -> 1 : (kkpp' = (kkpp + 1));
endmodule

module MAPKKK
	kkk: [0..N] init N;
	kkk_e1: [0..N] init 0;
	kkkp: [0..N] init 0;
	kkkp_e2: [0..N] init 0;
	<a_kkk_e1> ((kkk > 0) & (kkk_e1 < N)) -> (a1 * kkk) : (kkk' = (kkk - 1)) & (kkk_e1' = (kkk_e1 + 1));
	<d_kkk_e1> ((kkk < N) & (kkk_e1 > 0)) -> (d1 * kkk_e1) : (kkk' = (kkk + 1)) & (kkk_e1' = (kkk_e1 - 1));
	<k_kkk_e1> ((kkk_e1 > 0) & (kkkp < N)) -> (k1 * kkk_e1) : (kkk_e1' = (kkk_e1 - 1)) & (kkkp' = (kkkp + 1));
	<a_kkk_e2> ((kkkp > 0) & (kkkp_e2 < N)) -> (a2 * kkkp) : (kkkp' = (kkkp - 1)) & (kkkp_e2' = (kkkp_e2 + 1));
	<d_kkk_e2> ((kkkp < N) & (kkkp_e2 > 0)) -> (d2 * kkkp_e2) : (kkkp' = (kkkp + 1)) & (kkkp_e2' = (kkkp_e2 - 1));
	<k_kkk_e2> ((kkkp_e2 > 0) & (kkk < N)) -> (k2 * kkkp_e2) : (kkk' = (kkk + 1)) & (kkkp_e2' = (kkkp_e2 - 1));
	<a_kk_kkk> (kkkp > 0) -> kkkp : (kkkp' = (kkkp - 1));
	<d_kk_kkk> (kkkp < N) -> 1 : (kkkp' = (kkkp + 1));
	<k_kk_kkk> (kkkp < N) -> 1 : (kkkp' = (kkkp + 1));
endmodule

