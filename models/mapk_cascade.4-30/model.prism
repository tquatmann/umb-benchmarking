module E1
	e1: [0..E1] init E1;
	<a_kkk_e1> (e1 > 0) -> e1 : (e1' = (e1 - 1));
	<d_kkk_e1> (e1 < E1) -> 1 : (e1' = (e1 + 1));
	<k_kkk_e1> (e1 < E1) -> 1 : (e1' = (e1 + 1));
endmodule

